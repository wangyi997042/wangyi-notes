# 环境标识
NODE_ENV = "production"
VUE_APP_ENV = "production"
# 生产环境接口域名（打包后直接请求）
VUE_APP_API_BASE_URL = "https://prod-api.example.com"
# 生产环境打包输出目录（默认dist，可自定义）
VUE_APP_OUTPUT_DIR = "dist-prod"
# 生产环境是否开启 sourcemap（建议关闭）
VUE_APP_SOURCEMAP = false
# 生产环境公共路径（如部署到子路径：/vue-project/）
VUE_APP_PUBLIC_PATH = "/"